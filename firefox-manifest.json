{
  "manifest_version": 2,

  "browser_specific_settings": {
    "gecko": {
      "id": "new-tab@miyuun.com",
      "strict_min_version": "110.0"
    }
  },

  "name": "New Tab (public beta)",
  "version": "0.1",
  "description": "Replaced default new tab page with a customizable page",

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": ["storage", "activeTab", "scripting", "tabs"],

  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",

  "content_scripts": [
    {
      "matches": ["<all_urls>"]
    }
  ]
}
